{
  "private": true,
  "name": "browser-app",
  "version": "0.0.0",
  "dependencies": {
    "@theia/core": "1.58.1",
    "@theia/preferences": "1.58.1",
    "CheckingApp": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "1.58.1"
  },
