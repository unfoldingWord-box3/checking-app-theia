{
  "private": true,
  "name": "electron-app",
  "version": "0.0.0",
  "main": "lib/backend/electron-main.js",
  "dependencies": {
    "@theia/core": "1.58.1",
    "@theia/electron": "1.58.1",
    "@theia/messages": "1.58.1",
    "@theia/mini-browser": "1.58.1",
    "@theia/navigator": "1.58.1",
    "@theia/outline-view": "1.58.1",
    "@theia/preferences": "1.58.1",
    "@theia/workspace": "1.58.1",
    "CheckingApp": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "1.58.1",
    "electron": "30.5.1"
  },
  "scripts": {
    "bundle": "yarn rebuild && theia build --mode development",
    "rebuild": "theia rebuild:electron --cacheRoot ..",
    "start": "theia start",
    "watch": "yarn rebuild && theia build --watch --mode development"
  },
  "theia": {
    "target": "electron"
  }
}
